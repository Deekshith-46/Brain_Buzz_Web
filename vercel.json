{
  "version": 2,
  "functions": {
    "src/server.js": {
      "runtime": "@vercel/node",
      "maxDuration": 60,
      "memory": 1024,
      "maxLambdaSize": "50mb"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "content-length": "0"
      }
    }
  ]
}
